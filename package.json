    "@mantine/next": "^4.2.11",
    "@prisma/client": "^4.0.0",
    "apollo-server-micro": "^3.9.0",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.3",
    "formik": "^2.2.9",
    "graphql": "^15.5.0",
    "graphql-request": "^4.3.0",
    "micro": "^9.3.4",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
