    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "dotenv": "^16.0.1",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
